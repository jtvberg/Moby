{
  "name": "Moby",
  "version": "0.9.9",
  "description": "It will task you",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "debug": "electron --inspect=6969 .",
    "break": "electron --inspect-brk=6969 .",
    "build": "electron-builder -m",
    "release": "electron-builder -m -p 'onTagOrDraft'"
  },
  "repository": "https://github.com/jtvberg/Moby",
  "homepage": "https://github.com/jtvberg/Moby",
  "keywords": [
    "Electron",
    "task",
    "Kanban"
  ],
  "author": "jtvberg",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.3.2",
    "electron-builder": "^22.9.1",
    "electron-log": "^4.2.4",
    "electron-notarize": "^1.0.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@octokit/rest": "^18.0.6",
    "bootstrap": "^4.5.3",
    "custom-electron-titlebar": "^3.2.5",
    "electron-updater": "^4.3.5",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1",
    "rally": "^2.1.3",
    "servicenow-rest-api": "^1.1.1"
  },
  "build": {
    "appId": "com.jtvberg.moby",
    "publish": {
      "provider": "github"
    },
    "afterSign": "./build/afterSign.js",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "certificateFile": "private/jtvberg.p12",
      "certificatePassword": "",
      "verifyUpdateCodeSignature": false
    },
    "linux": {
      "category": "Utility"
    }
  }
}
